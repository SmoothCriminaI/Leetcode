class Solution {//0ms,100%
public:
    //G(n) = F(1, n) + F(2, n) + ... + F(n, n). 
    //F(i, n) = G(i-1) * G(n-i)	1 <= i <= n 
    //G(n) = G(0) * G(n-1) + G(1) * G(n-2) + â€¦ + G(n-1) * G(0) 
    int numTrees(int n) {
        if(n==0)return 1;
        if(n==1)return 1;
        int ans[n+1]={0};
        ans[0]=1;ans[1]=1;
        for(int i =2;i<=n;i++)
        {
            for(int j = 1;j<=i;j++)
            {
                ans[i]+=ans[j-1]*ans[i-j];
            }
        }
        return ans[n];
    }
};
