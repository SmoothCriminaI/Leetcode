       /* long i = 1,sum = 0;
        for(i=1;i<=max(2,n/2);i++)
        {
            if(sum+i>n)
                break;
            sum+=i;
        }
        return (int)i-1;*/
        return floor(-0.5+sqrt((double)2*n+0.25));
    }
